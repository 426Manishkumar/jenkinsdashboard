pipeline {
    agent any

    stages {
        stage('Checkout') {
            steps {
                echo 'Cloning repository...'
                git 'https://github.com/426Manishkumar/jenkinsdashboard.git'
            }
        }

        stage('Build') {
            steps {
                echo 'Build stage running...'
            }
        }

        stage('Run') {
            steps {
                echo 'Application running successfully!'
            }
        }
    }
}
